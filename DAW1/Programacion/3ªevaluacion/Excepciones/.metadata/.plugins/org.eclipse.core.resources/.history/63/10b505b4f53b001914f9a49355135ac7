package ejercicio2;

public class Reloj implements Mostrable{

	private int hora;
	private int minutos;
	private int segundos;
	private Formato formato;
	
	public Reloj(int hora, int minutos, int segundos, Formato formato) {
		super();
		this.hora = hora;
		this.minutos = minutos;
		this.segundos = segundos;
		this.formato = formato;
	}

	public int getHora() {
		return hora;
	}

	public int getMinutos() {
		return minutos;
	}

	public int getSegundos() {
		return segundos;
	}

	@Override
	public String toString(Object o) {
		// TODO Auto-generated method stub
		return null;
	}
	
	
	
	
}
