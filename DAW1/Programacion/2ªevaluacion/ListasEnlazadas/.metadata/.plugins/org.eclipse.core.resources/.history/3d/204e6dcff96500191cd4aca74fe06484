
public class Prueba {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		List list = new List();
		int count = 3;
		for(int i = 0; i < count; i++) {
			list.insertAfter(InputData.readInteger());
		}
		System.out.println(list.showContent());
		list.invertir();
		System.out.println(list.showContent());
		
	}

}
